{
  "name": "ergebnis/day-one-to-obsidian-converter",
  "description": "Provides a console command for converting DayOne journals to Obsidian notes.",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/day-one-to-obsidian-converter",
  "support": {
    "issues": "https://github.com/ergebnis/day-one-to-obsidian-converter/issues",
    "source": "https://github.com/ergebnis/day-one-to-obsidian-converter"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0",
    "ergebnis/json-schema-validator": "^2.0.0",
    "symfony/console": "^6.3.0",
    "symfony/filesystem": "^6.3.1",
    "symfony/yaml": "^6.3.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.29.0",
    "ergebnis/data-provider": "^2.0.0",
    "ergebnis/license": "^2.1.0",
    "ergebnis/php-cs-fixer-config": "^5.8.0",
    "fakerphp/faker": "^1.23.0",
    "infection/infection": "~0.27.0",
    "phpunit/phpunit": "^10.2.2",
    "psalm/plugin-phpunit": "~0.18.4",
    "rector/rector": "~0.17.1",
    "vimeo/psalm": "^5.11"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\DayOneToObsidianConverter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\DayOneToObsidianConverter\\Test\\": "test/"
    }
  },
  "bin": [
    "bin/day-one-to-obsidian-converter"
  ],
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.1.8"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
