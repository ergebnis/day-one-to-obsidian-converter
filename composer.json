{
  "name": "ergebnis/day-one-to-obsidian-converter",
  "description": "Provides a console command for converting DayOne journals to Obsidian notes.",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/day-one-to-obsidian-converter",
  "support": {
    "issues": "https://github.com/ergebnis/day-one-to-obsidian-converter/issues",
    "source": "https://github.com/ergebnis/day-one-to-obsidian-converter"
  },
  "require": {
    "php": "^8.1",
    "ergebnis/json-schema-validator": "^2.0.0",
    "symfony/console": "^6.1.4",
    "symfony/filesystem": "^6.1.4",
    "symfony/finder": "^6.1.3",
    "symfony/yaml": "^6.1.4"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.28.3",
    "ergebnis/data-provider": "^1.2.0",
    "ergebnis/license": "^1.2.0",
    "ergebnis/php-cs-fixer-config": "^4.7.0",
    "fakerphp/faker": "^1.20.0",
    "infection/infection": "~0.26.13",
    "phpunit/phpunit": "^9.5.24",
    "psalm/plugin-phpunit": "~0.17.0",
    "vimeo/psalm": "^4.26"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\DayOneToObsidianConverter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\DayOneToObsidianConverter\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.1.8"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
